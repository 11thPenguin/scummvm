--
set x = 1
if x = 5 then exit
else put 10.0

set y = 4

repeat with x = 1 to 6
	if x = 3 then put 30
	else if x = 4 then put 40
	else if x = 5 then put 50
	else put 10.0
	if x = 1 then
	  put 1
	else if x = 2 then
	  put 1232.12345678901234
	  put 2.2
	else if x = 3 then
		put 3
	else if x = 4 then
	  put 4
	else if x = 5 then
	  put 5
  	else if x = 6 then
	  put 6
	end if
	if x = 4 then put 40
	else put 50
end repeat

    if the keyCode = 36 then                    --  return key goes to first menu
        go to frame 1
    else if the keyCode = 123 then           --  left arrow goes to previous menu
        dontPassEvent
        go to marker(-1)
    else if the keyCode = 124 then           --  right arrow goes to next menu
        dontPassEvent
        go to marker(1)
    else if the keyCode = 125 then           --  down arrow goes to last (bottom) menu
        dontPassEvent
        go to frame "credits"
    else if the keyCode = 126 then           --  up arrow goes to first (top) menu
        dontPassEvent
        go to frame 1
    end if

if abs(y) = x and abs(x) = y then
  put TRUE
else
  put FALSE
end if
